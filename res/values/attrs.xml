<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <declare-styleable name="SliderPreference">
        <attr name="minText" format="string" />
        <attr name="maxText" format="string" />
    </declare-styleable>
    <declare-styleable name="KeyConfigPreference">
        <attr name="leftKey" format="string" />
        <attr name="rightKey" format="string" />
        <attr name="jumpKey" format="string" />
        <attr name="attackKey" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomFontTextView">
        <attr name="setFont" format="string" />
        <attr name="addSpacing" format="boolean" />
    </declare-styleable>
    <declare-styleable name="Options">
        <attr name="set_title" format="string" />
        <attr name="set_title_color" format="color" />
        <attr name="set_description" format="string" />
        <attr name="set_description_color" format="color" />
    </declare-styleable>
    <declare-styleable name="OptionsCheckbox">
        <attr name="set_on_text" format="string" />
        <attr name="set_off_text" format="string" />
        <attr name="set_mid_text" format="string" />
        <attr name="set_tristate" format="boolean" />
        <attr name="set_on_text_color" format="color" />
        <attr name="set_off_text_color" format="color" />
        <attr name="set_mid_text_color" format="color" />
    </declare-styleable>
    <declare-styleable name="OptionsSlider">
        <attr name="set_max" format="integer" />
        <attr name="set_pos" format="integer" />
        <attr name="set_min_text" format="string" />
        <attr name="set_max_text" format="string" />
    </declare-styleable>

</resources>